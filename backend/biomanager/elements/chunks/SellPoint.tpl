<tr city="[[+city]]" class="cont_list">
    <td class="sellitem__name">[[+name]]</td>
    <td class="sellitem__address" geo_code="[[+geo]],[[+lat]]">[[+adress]]</td>
    <td class="sellitem__url">[[+desc:ne=`0`:then=`<a href="[[+desc]]" target="_blank">[[+desc]]</a>`:else=`Звоните`]]</td>
    <td class="sellitem__phone">[[+phone]]</td>
</tr>